{% block statistics %}
    <div class="statistics">
        <div class="statistic-item">
            <div class="statistic-value" aria-label="Time">{{ post.time }}</div>
        </div>
        <div class="statistic-item"><span class="statistic-icon"><svg viewBox="0 0 24 24"><path
                d="M2.002 6a3 3 0 0 1 3-3h14a3 3 0 0 1 3 3v10a3 3 0 0 1-3 3H12.28l-4.762 2.858A1 1 0 0 1 6.002 21v-2h-1a3 3 0 0 1-3-3V6Zm3-1a1 1 0 0 0-1 1v10a1 1 0 0 0 1 1h2a1 1 0 0 1 1 1v1.234l3.486-2.092a1 1 0 0 1 .514-.142h7a1 1 0 0 0 1-1V6a1 1 0 0 0-1-1h-14Z"></path></svg></span><span
                class="statistic-value" aria-label="Comments">{{ post.comments }}</span></div>
        <div class="statistic-item"><span class="statistic-icon"><svg viewBox="0 0 24 24"><path
                d="M16.734 5.091c-1.238-.276-2.708.047-4.022 1.38a1 1 0 0 1-1.424 0C9.974 5.137 8.504 4.814 7.266 5.09c-1.263.282-2.379 1.206-2.92 2.556C3.33 10.18 4.252 14.84 12 19.348c7.747-4.508 8.67-9.168 7.654-11.7-.541-1.351-1.657-2.275-2.92-2.557Zm4.777 1.812c1.604 4-.494 9.69-9.022 14.47a1 1 0 0 1-.978 0C2.983 16.592.885 10.902 2.49 6.902c.779-1.942 2.414-3.334 4.342-3.764 1.697-.378 3.552.003 5.169 1.286 1.617-1.283 3.472-1.664 5.17-1.286 1.927.43 3.562 1.822 4.34 3.764Z"></path></svg></span><span
                class="statistic-value" aria-label="Likes">{{ post.likes }}</span></div>
        <div class="statistic-item"><span class="statistic-icon"><svg viewBox="0 0 24 24"><path
                d="M12.781 2.375c-.381-.475-1.181-.475-1.562 0l-8 10A1.001 1.001 0 0 0 4 14h4v7a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1v-7h4a1.001 1.001 0 0 0 .781-1.625l-8-10zM15 12h-1v8h-4v-8H6.081L12 4.601 17.919 12H15z"></path></svg></span><span
                class="statistic-value" aria-label="Up Votes">{{ post.up_votes }}</span></div>
        <div class="statistic-item"><span class="statistic-icon"><svg viewBox="0 0 24 24"><path
                d="M20.901 10.566A1.001 1.001 0 0 0 20 10h-4V3a1 1 0 0 0-1-1H9a1 1 0 0 0-1 1v7H4a1.001 1.001 0 0 0-.781 1.625l8 10a1 1 0 0 0 1.562 0l8-10c.24-.301.286-.712.12-1.059zM12 19.399 6.081 12H10V4h4v8h3.919L12 19.399z"></path></svg></span><span
                class="statistic-value" aria-label="Down Votes">{{ post.down_votes }}</span></div>
    </div>
{% endblock %}
